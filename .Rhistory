fill = "red", alpha = 0.2
) +
scale_x_continuous(labels = scales::date_format("%Y-%m-%d"), breaks = scales::date_breaks("1 year"), limits = c(min(database$Date_num), max_date_num)) +
theme_light(base_size = 15) +
